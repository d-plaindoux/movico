model number
model string

model Point {
    x : number
    y : number
    c : string
}

type Peano {
    model Zero
    model Succ { _ : Peano }
}

class peano this:Peano {
    unbox : Peano
    (+) : peano -> peano
} {
    def unbox = this
    def Zero.(+) p = self
    def Succ.(+) p = 
        let p = self this._ + p in
            self $ Succ $ p unbox
}