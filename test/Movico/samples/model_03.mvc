model number {
    (+) : number -> number
}

type Expr {
    model Int {
        _ : number
    }
    model Add {
        _1 : Expr
        _2 : Expr
    }
}

class expr this:Expr {
    eval : number
} {
    def Int.eval = this._
    def Add.eval = 
        let e1 = self this._1 eval in
        let e2 = self this._2 eval in
            e1 + e2
}

