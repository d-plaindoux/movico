model number {
    (+) : number -> number
}

type Expr {
    model Int {
        _ : number
    }
    model Add {
        _1 : Expr
        _2 : Expr
    }
}

class ExprEval this:Expr {
    eval : number
} {
    def (Int).eval = this._
    def (Add).eval = (self this._1).eval + (self this._2).eval
}

