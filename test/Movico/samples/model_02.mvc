// Simple example with a model and a view 

model Todo {
    value : string
}

class TodoClass this:Todo {
    what : string   
    change : string -> TodoClass
} {
    def what = this.value
    def change what = self (Todo what)
}

view TodoView this:TodoClass {
    <div> 
        <div class="todo" what=this.what/>
    </div>
}
