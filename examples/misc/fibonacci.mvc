module Misc
 
from Data.Number import number
from Data.Lang import when
 
//
// Fibonacci suite
//

def fib : number -> number = 
     fun n -> 
        when (n << 2) 
        then n
        else { (fib $ n - 1) + (fib $ n - 2) }
